                        <li><a href="/logout">Logout</a></li>
                    </div>

                </ul>
            </div></div>
    </div>
</nav>
<div class="notify">
    <div class = "backgroundImage">
        <label id="heading"></label>
    </div>
    <div  class="col-sm-5 col-sm-offset-4" class="container-fluid" style="margin-top: 50px;">
        <div ng-show="showFlag == true" ng-repeat="row in rows">
            <div align="left" class="panel panel-default">
                <!-- Default panel contents -->
                <div class="panel-heading"><strong>{{row.fullName}}</strong>@{{row.userHandler}}</div>
                <div class="panel-body">
                    <p>{{row.Msg}}</p>
                </div>
            </div>
        </div>
    </div>
